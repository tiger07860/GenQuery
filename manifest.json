{
    "manifest_version": 2,
    "name": "GenQuery",
    "version": "1.0",
    "description": "Make Queries on just a click!",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "contextMenus",
        "tts"
    ]
}